{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "jest": true
  },
  "extends": [
    // "airbnb",
    "airbnb/hooks",
    "prettier"
  ],
  "plugins": ["react", "prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "rules": {
    "react/jsx-filename-extension": "off",
    "object-curly-newline": "off",
    "react/require-default-props": "off",
    "arrow-body-style": "off"
  }

};
